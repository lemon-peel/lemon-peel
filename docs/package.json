{
  "name": "@element-plus/docs",
  "private": true,
  "scripts": {
    "dev": "pnpm gen-locale && vitepress dev .",
    "build": "NODE_ENV=production && vitepress build . && node -r @lemon-peel/register .vitepress/build/rebuildPwa.ts",
    "serve": "NODE_ENV=production vitepress serve . --port 5001",
    "gen-locale": "rimraf .vitepress/i18n && node -r @lemon-peel/register .vitepress/build/crowdinGenerate.ts",
    "crowdin-credentials": "node -r @lemon-peel/register .vitepress/build/crowdin-credentials.ts"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.9",
    "@lemon-peel/metadata": "workspace:*",
    "@vue/shared": "^3.3.4",
    "@vueuse/core": "^10.3.0",
    "axios": "^0.27.2",
    "clipboard-copy": "^4.0.1",
    "lemon-peel": "npm:lemon-peel@latest",
    "markdown-it": "^13.0.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "prism-theme-vars": "^0.2.3",
    "vue": "^3.3.4",
    "workbox-build": "^6.3.0",
    "workbox-core": "^6.3.0"
  },
  "devDependencies": {
    "@crowdin/cli": "^3.7.8",
    "@docsearch/css": "^3.5.1",
    "@docsearch/js": "^3.5.1",
