    "@iconify-json/ri": "^1.1.3",
    "@types/markdown-it": "^12.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "chalk": "^4.1.2",
    "consola": "^2.15.3",
    "escape-html": "^1.0.3",
    "fast-glob": "^3.2.11",
    "markdown-it-container": "^3.0.0",
    "octokit": "^2.0.3",
    "prismjs": "^1.28.0",
    "unocss": "^0.54.0",
    "unplugin-icons": "^0.16.5",
    "unplugin-vue-components": "^0.25.1",
    "unplugin-vue-macros": "^2.4.0",
    "vite": "^4.4.7",
