{
  "name": "@lemon-peel/unplugin-resolver",
  "version": "0.0.1",
  "description": "Resolver for Lemon-Peel basic on unplugin-vue-components",
  "keywords": [
    "lemon-peel",
    "unplugin resolver"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "pnpm run stub",
    "stub": "pnpm run build"
  },
  "dependencies": {
    "@pnpm/logger": "^4.0.0",
    "compare-versions": "^5.0.1",
    "consola": "^2.15.3",
    "local-pkg": "^0.4.2",
    "unplugin-vue-components": "^0.25.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "unplugin-vue-components": "^0.22.11"
  }
}
