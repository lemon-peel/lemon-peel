  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "chrome > 85",
    "firefox > 90",
    "not IE > 0",
    "not op_mini all"
  ],
  "packageManager": "pnpm@7.28.0",
  "license": "MIT"
