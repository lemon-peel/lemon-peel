    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/ui": "^0.31.0",
    "@vue/runtime-core": "^3.2.47",
    "@vue/test-utils": "^2.4.0",
    "chalk": "^5.0.1",
    "concurrently": "^7.5.0",
    "consola": "^2.15.3",
    "csstype": "^2.6.20",
    "eslint": "^8.30.0",
