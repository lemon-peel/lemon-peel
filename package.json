{
  "description": "An ui framework based on vue3.",
  "keywords": [
    "vue",
    "vue3",
    "design",
    "vueComponent",
    "component",
    "components",
    "ui",
    "framework",
    "frontend"
  ],
  "author": "jason-chang <chaoyue.chang@gmail.com>",
  "homepage": "https://lemon-peel.lemonbot.fun/",
  "main": "index.js",
  "scripts": {
    "cz": "czg",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest --coverage",
    "test:ssr": "vitest --config ./ssr-testing/vitest.config.ts",
    "prepare:e2e": "if [ ! -d \"docs/.vitepress/dist\" ]; then pnpm run docs:build; fi;",
    "dev": "pnpm -C serve dev",
    "gen": "bash ./scripts/gc.sh",
    "gen:version": "node -r @lemon-peel/register scripts/genVersion.ts",
    "diff:table": "node -r @lemon-peel/register scripts/build-table.ts",
    "update:version": "node -r @lemon-peel/register scripts/updateVersion.ts",
    "clean": "pnpm run clean:dist && pnpm run -r --parallel clean",
    "clean:dist": "rimraf dist",
    "build": "pnpm run -C toolkit/build start",
    "build:theme": "pnpm run -C packages/theme-chalk build",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md --max-warnings 0 --cache",
    "lint:watch": "esw -w . --ext .vue,.js,.ts,.jsx,.tsx,.md --max-warnings 0 --cache --watch-delay 5000",
    "lint:fix": "pnpm run lint --fix",
    "lint:commit": "commitlint --from $(git merge-base origin/dev HEAD) --to HEAD > ./commit-lint.txt",
    "typecheck": "run-p typecheck:web typecheck:play typecheck:node typecheck:vite-config typecheck:vitest",
    "typecheck:web": "vue-tsc -p tsconfig.web.json --composite false --noEmit",
    "typecheck:node": "tsc -p tsconfig.node.json --noEmit",
    "typecheck:play": "vue-tsc -p tsconfig.play.json --composite false --noEmit",
    "typecheck:vite-config": "vue-tsc -p tsconfig.vite-config.json --composite false --noEmit",
    "typecheck:vitest": "vue-tsc -p tsconfig.vitest.json --composite false --noEmit",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build",
    "docs:serve": "pnpm run -C docs serve",
    "docs:gen-locale": "pnpm run -C docs gen-locale",
    "docs:crowdin-credentials": "pnpm run -C docs crowdin-credentials",
    "stub": "pnpm run -r --parallel stub",
    "tsc": "vue-tsc --declaration --emitDeclarationOnly --project tsconfig.pkgs.json --outDir dist/types",
    "prepare": "husky install",
    "postinstall": "pnpm stub && concurrently \"pnpm gen:version\" \"pnpm run -C toolkit/metadata dev\""
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "peerDependencies": {
    "vue": ">=3.3.4"
  },
  "dependencies": {
    "@ctrl/tinycolor": "^3.4.1",
    "@element-plus/icons-vue": "^2.0.6",
    "@floating-ui/dom": "^1.0.1",
    "@lemon-peel/components": "workspace:*",
    "@lemon-peel/constants": "workspace:*",
    "@lemon-peel/directives": "workspace:*",
    "@lemon-peel/hooks": "workspace:*",
    "@lemon-peel/locale": "workspace:*",
    "@lemon-peel/test-utils": "workspace:*",
    "@lemon-peel/theme-chalk": "workspace:*",
    "@lemon-peel/tokens": "workspace:*",
    "@lemon-peel/utils": "workspace:*",
    "@popperjs/core": "^2.11.6",
    "@vitest/browser": "^0.31.0",
    "@vue/shared": "^3.3.4",
    "@vueuse/core": "^10.3.0",
    "async-validator": "^4.2.5",
    "dayjs": "^1.11.5",
    "escape-html": "^1.0.3",
    "lemon-peel": "workspace:*",
    "lodash": "^4.17.21",
    "memoize-one": "^6.0.0",
    "normalize-wheel-es": "^1.2.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.0",
    "@commitlint/config-conventional": "^17.7.0",
    "@lemon-peel/build-utils": "workspace:*",
    "@lemon-peel/register": "workspace:*",
    "@lemon-peel/unplugin-resolver": "workspace:*",
    "@lemonbot.fun/eslint-config-vue": "^2.6.0",
    "@pnpm/find-workspace-packages": "^4.0.16",
    "@pnpm/types": "^8.9.0",
    "@swc-node/register": "^1.6.6",
    "@types/escape-html": "^1.0.2",
    "@types/gulp": "^4.0.10",
    "@types/lodash": "^4.14.197",
    "@types/node": "^18.11.9",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/runtime-core": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "chalk": "^4.1.2",
    "concurrently": "^7.5.0",
    "consola": "^2.15.3",
    "csstype": "^2.6.20",
    "eslint": "^8.30.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-watch": "^8.0.0",
    "expect-type": "^0.15.0",
    "fast-glob": "^3.2.12",
    "gulp": "^4.0.2",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "pinia": "^2.0",
    "puppeteer": "^21.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "rimraf": "^5.0",
    "sass": "^1.66.1",
    "tslib": "^2.4.0",
    "type-fest": "^3.3.0",
    "typescript": "^4.9.5",
    "unplugin-vue-define-options": "^1.1.1",
    "unplugin-vue-macros": "^2.4.0",
    "vitest": "^0.32.2",
    "vue-router": "^4.2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "chrome > 85",
    "firefox > 90",
    "not KaiOS > 0",
    "not IE > 0",
    "not op_mini all"
  ],
  "packageManager": "pnpm@8.6.12",
  "license": "MIT"
}
